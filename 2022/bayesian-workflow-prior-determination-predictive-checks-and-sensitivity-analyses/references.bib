@article{harnadSymbolGroundingProblem1990,
  title = {The Symbol Grounding Problem},
  author = {Harnad, Stevan},
  date = {1990-06-01},
  journaltitle = {Physica D: Nonlinear Phenomena},
  shortjournal = {Physica D: Nonlinear Phenomena},
  volume = {42},
