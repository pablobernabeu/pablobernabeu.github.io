@article{taikhVisibleSemanticPrimes2020,
  title = {Do Visible Semantic Primes Preactivate Lexical Representations?},
  author = {Taikh, Alexander and Lupker, Stephen J.},
  date = {2020},
  journaltitle = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
