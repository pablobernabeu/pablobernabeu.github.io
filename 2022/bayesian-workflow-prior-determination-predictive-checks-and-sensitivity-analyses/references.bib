@article{vergallitoPerceptualModalityNorms2020,
  title = {Perceptual Modality Norms for 1,121 {{Italian}} Words: {{A}} Comparison with Concreteness and Imageability Scores and an Analysis of Their Impact in Word Processing Tasks},
