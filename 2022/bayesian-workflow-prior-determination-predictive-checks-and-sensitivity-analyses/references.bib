@article{hutchisonPredictingSemanticPriming2008,
  title = {Predicting Semantic Priming at the Item Level},
  author = {Hutchison, K.A. and Balota, David A. and Cortese, Michael J. and Watson, Jason M.},
  date = {2008-07},
  journaltitle = {Quarterly Journal of Experimental Psychology},
  shortjournal = {Quarterly Journal of Experimental Psychology},
  volume = {61},
